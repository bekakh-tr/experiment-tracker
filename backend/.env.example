# App
APP_NAME=Experiment Tracker
APP_ENV=local
ALLOWED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080

# Databricks OAuth profile from ~/.databrickscfg (used for access token + host)
DBX_PROFILE=Beka Databricks BI

# Fallback profile used only to resolve SQL warehouse http_path when OAuth profile has no http_path.
DBX_HTTP_PATH_PROFILE=BekaLocal

# Optional: path to Databricks config file
DBX_CONFIG_FILE=~/.databrickscfg

# Mixpanel silver data contract
DBX_TABLE=main.mixpanel.silver
DBX_EVENT_NAME_COLUMN=mp_event_name
DBX_EVENT_NAME_VALUE=$experiment_started
DBX_PARTITION_YEAR_COLUMN=etr_y
DBX_PARTITION_MONTH_COLUMN=etr_ym
DBX_PARTITION_DAY_COLUMN=etr_ymd
DBX_GCID_COLUMN=mp_user_id
DBX_EVENT_TS_COLUMN=UpdateDate
DBX_EXPERIMENT_ID_COLUMN=x_experiment_id_blob
DBX_EXPERIMENT_NAME_COLUMN=experiment_name
DBX_VARIANT_COLUMN=variationid
DBX_VARIATION_BLOB_COLUMN=x_variation_id_blob
